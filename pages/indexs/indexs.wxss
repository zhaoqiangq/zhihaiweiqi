/* pages/indexs/indexs.wxss */
page{
  height: 100%;
}
swiper{
  width: 100%;
  height: 450rpx;
}
swiper-item image{
  width: 100%;
}
.itemimg{
  padding:0 10rpx;
  box-sizing: border-box;
}
.itemimg image{
  width: 100%;
}
.nav{
  width: 100%;
  height: 180rpx;
  margin: 0 auto;
  position: relative;
}
.nav navigator{
  width: 33%;
  display: inline-block;
  height: 100%;
}
.nav navigator view{
  font-size: 25rpx;
  color: #333;
  text-align: center;
  padding-top: 30rpx;
  box-sizing: border-box;
}
.nav navigator view image{
  margin: 0 auto;
  width: 70rpx;
  display: block;
  margin-bottom: 10rpx;
}